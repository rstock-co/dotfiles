{
    "workbench.colorTheme": "One Dark Pro Mix",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "window.zoomLevel": 1,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.minimap.enabled": false,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "'BPmono'",
    "workbench.colorCustomizations": {
        "editor.background": "#020109",
        "editor.selectionHighlightBackground": "#1b1336",
        "editor.selectionHighlightBorder":"#300eb7",
        "editor.selectionBackground": "#322448",
        "sideBar.background": "#0e0e1a",
        "activityBar.background": "#060914",
        "sideBar.foreground": "#a2a0bc",
        "sideBarTitle.foreground": "#746ade",
        "sideBarSectionHeader.border": "#110f26",
        "tab.activeBackground": "#0e0e1a",
        "tab.inactiveBackground": "#191928",
        "tab.activeBorder":"#6d5a89",
        "tab.activeBorderTop":"#423554",
        "tab.activeForeground": "#bfbcdf",
        "tab.border":"#2d2a39",
        "tab.hoverBackground":"#242436"
        

    },
    "sass.disableAutoIndent": true,
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "scss.showErrors": true
}